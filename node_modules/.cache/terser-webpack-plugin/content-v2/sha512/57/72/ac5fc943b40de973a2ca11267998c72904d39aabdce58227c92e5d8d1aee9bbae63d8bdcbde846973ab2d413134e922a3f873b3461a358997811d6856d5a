;\\n    },\\n    fadeIn: function (element, options) {\\n        let to = 0;\\n        this.animate({\\n            duration: options.duration,\\n            delta: function (progress) {\\n                progress = this.progress;\\n                return FX.easing.swing(progress);\\n            },\\n            step: function (delta) {\\n                element.style.opacity = to + delta;\\n            }\\n        });\\n    }\\n};\\n\\nclass Button extends Component {\\n    constructor(props) {\\n        super(props);\\n        this.handleOnClick = this.handleOnClick.bind(this);\\n    }\\n\\n    handleOnClick() {\\n        this.props.getQuote();\\n        this.props.changeTheme();\\n        FX.fadeIn(document.getElementById('quote-content'), {\\n            duration: 2000\\n        });\\n        FX.fadeIn(document.getElementById('quote-author'), {\\n            duration: 2000\\n        });\\n    }\\n\\n    render() {\\n        let {buttonColor} = this.props;\\n\\n        return (\\n            <div className=\\\"row\\\">\\n                <button\\n                    className=\\\"btn btn-quote background\\\"\\n                    style={\\n                        {\\n                            float: 'right',\\n                            marginRight: '2%',\\n                            marginBottom: '2%',\\n                            background: buttonColor,\\n                            color: 'white'\\n                        }\\n                    }\\n                    onClick={this.handleOnClick}\\n                >New quote\\n                </button>\\n            </div>\\n        );\\n    }\\n}\\n\\nexport default Button;\",\"import React from 'react';\\nimport Button from '../Button/Button'\\n\\nconst Panel = (props) => {\\n    let {themeColor, panelColor, textColor, data, changeTheme, getQuote} = props;\\n    return (\\n        <div\\n            className=\\\"panel panel-default col-xs-4 col-xs-offset-4\\\"\\n            style={{backgroundColor:pa